{
  "name": "tuw-autojoin",
  "version": "0.0.1",
  "main": "./build/index.js",
  "type": "module",
  "private": "true",
  "license": "All rights reserved.",
  "author": "Johannes Garz <johannes@garz.dev> (https://garz.dev/)",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc && tsc-alias",
    "start": "node ./build/index.js",
    "deploy": "yarn build && yarn start",
    "dev": "dotenv -e .env -v NODE_ENV=development tsx watch --clear-screen=false ./src/index.ts",
    "ncu": "ncu -u",
    "lint": "prettier --check . '!./{,data}' && eslint",
    "format": "prettier -w . '!./{,data}' && eslint --fix"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/node-schedule": "^2.1.7",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "npm-check-updates": "^17.1.3",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "node-schedule": "^2.1.1",
    "puppeteer": "^23.5.1",
    "zod": "^3.23.8"
  }
}
